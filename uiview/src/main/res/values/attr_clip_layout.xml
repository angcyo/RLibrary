<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClipLayout">
        <attr name="r_clip_type" format="enum">
            <enum name="CLIP_TYPE_NONE" value="0"/>
            <enum name="CLIP_TYPE_DEFAULT" value="1"/>
            <enum name="CLIP_TYPE_ROUND" value="2"/>
            <enum name="CLIP_TYPE_CIRCLE" value="3"/>
            <enum name="CLIP_TYPE_RECT" value="4"/>
        </attr>
        <attr name="r_clip_radius" format="dimension"/>
        <attr name="r_background"/>
        <!--宽高是否相等-->
        <attr name="r_is_aeq_width"/>
    </declare-styleable>
</resources>