<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/base_white">
    <!--android:id="@android:id/mask" 限制波纹在 指定的Drawable内 -->
    <item android:drawable="@drawable/base_white_round_border_shape_little"/>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/base_round_little_radius"/>
            <solid android:color="@color/base_white"/>
        </shape>
    </item>
</ripple>
