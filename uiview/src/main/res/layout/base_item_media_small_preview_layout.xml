<?xml version="1.0" encoding="utf-8"?>
<com.angcyo.uiview.viewgroup.RFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="60dp"
    android:layout_height="60dp"
    android:layout_margin="@dimen/base_hdpi"
    tools:background="@color/transparent_dark20"
    tools:r_inner_border_color="@color/base_red"
    tools:r_show_inner_border="true">

    <com.angcyo.uiview.widget.GlideImageView
        android:id="@+id/base_image_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>

</com.angcyo.uiview.viewgroup.RFrameLayout>